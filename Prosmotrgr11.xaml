<Window x:Class="SKLAD.Prosmotrgr11"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SKLAD"
        mc:Ignorable="d"
        Title="Prosmotrgr11" Height="450" Width="800">
    <Grid Background="White">
        <Button Content="Назад" Click="exit" HorizontalAlignment="Left" Margin="30,385,0,0" VerticalAlignment="Top" Background="#FF3A90C9"/>
        <DataGrid x:Name="readers" AutoGenerateColumns="False" Margin="0,0,0,52" Background="White">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Номер" Binding="{Binding ID}"/>
                <DataGridTextColumn Header="Фамилия" Binding="{Binding Фамилия}"/>
                <DataGridTextColumn Header="Имя" Binding="{Binding Имя}"/>
                <DataGridTextColumn Header="Отчество" Binding="{Binding Отчество}"/>
                <DataGridTextColumn Header="Номер телефона" Binding="{Binding [Номер телефона]}"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
